<%- include("./partials/header") -%>


	<% if(notFound !== null) { %>
	
	<h1 class = "notFound"><%=notFound%> <%=searchedCar.year%> <%=searchedCar.make%> <%=searchedCar.parts%></h1>
	<button type="button" class="btn btn-dark"><a href = "/partSearch">Try again</a></button>
	
	<% } else { %>
	<div class = "container">
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Year</th>
      <th scope="col">Make/Model</th>
      <th scope="col">Part</th>
    </tr>
  </thead>
  <tbody>
	 <% car.forEach(function(car){ %>
    <tr>
      <td><%=car.year%></td>
      <td><%=car.make%></td>
	
		<td><a href = "/showParts/<%=car._id%>">Show parts</a>
	
		
		<% if(currentUser){ %>
		<form action = "/showParts/<%=car._id%>?_method=DELETE" method = "POST"><button  class="btn btn-danger">Delete</button></form></td>
		<%	} %>
    </tr>
	 <% }) %>
  </tbody>
</table>
	
</div>
	
	<% } %>




<%- include("./partials/footer") -%>
